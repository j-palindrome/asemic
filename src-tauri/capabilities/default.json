{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    "fs:allow-app-write-recursive",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text",
    "websocket:default"
  ]
}