{
  "name": "asemic",
  "version": "1.0.0",
  "description": "A TypeScript library for generative asemic writing",
  "type": "module",
  "main": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/server/server.ts",
    "build": "npm run build:css && npm run build:lib",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:types": "tsc --project tsconfig.build.json",
    "build:watch": "npm run build:lib -- --watch",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run test && npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@csound/browser": "^6.18.7",
    "@elemaudio/core": "^4.0.1",
    "@elemaudio/web-renderer": "^4.0.3",
    "@tailwindcss/cli": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node-osc": "^9.1.0",
    "@types/react": "^19.1.8",
    "@types/three": "^0.176.0",
    "@types/ws": "^8.18.1",
    "@webgpu/types": "^0.1.61",
    "canvas-record": "^5.3.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.515.0",
    "media-codecs": "^2.0.2",
    "mp4-muxer": "^5.2.2",
    "node-osc": "^9.1.4",
    "pts": "^0.12.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2",
    "regex-to-strings": "^2.1.0",
    "sharp": "^0.34.3",
    "simplex-noise": "^4.0.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "supercolliderjs": "^1.0.1",
    "tailwindcss": "^4.1.10",
    "test": "link:@types/playwright/test",
    "three": "^0.176.0",
    "tiny-invariant": "^1.3.3",
    "ws": "^8.18.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.16",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.4",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
