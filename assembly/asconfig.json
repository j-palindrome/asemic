{
  "extends": "./node_modules/assemblyscript/std/assembly.json",
  "targets": {
    "release": {
      "outFile": "build/index.wasm",
      "textFile": "build/index.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": true,
      "noAssert": true
    },
    "debug": {
      "outFile": "build/index.debug.wasm",
      "textFile": "build/index.debug.wat",
      "sourceMap": true,
      "debug": true
    }
  },
  "options": {
    "bindings": "esm",
    "exportRuntime": true,
    "runtime": "stub"
  }
}